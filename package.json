{
  "name": "devops-dashboard",
  "homepage": "https://marvin-kaiser.dev/devops-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-regular-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@reduxjs/toolkit": "1.9.7",
    "@svgr/webpack": "8.1.0",
    "axios": "1.6.7",
    "browserslist": "4.23.0",
    "camelcase": "8.0.0",
    "dotenv": "16.4.5",
    "dotenv-expand": "11.0.6",
    "fontfaceobserver": "2.3.0",
    "fs-extra": "11.2.0",
    "identity-obj-proxy": "3.0.0",
    "inquirer": "9.2.15",
    "inquirer-directory": "2.2.0",
    "localforage": "1.10.0",
    "moment": "2.30.1",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-bootstrap": "2.10.1",
    "react-dom": "18.2.0",
    "react-grid-layout": "1.4.4",
    "react-helmet-async": "2.0.4",
    "react-markdown": "9.0.1",
    "react-redux": "9.1.0",
    "react-refresh": "0.14.0",
    "react-router-dom": "6.22.2",
    "redux-injectors": "2.1.0",
    "redux-saga": "1.3.0",
    "rehype-sanitize": "6.0.0",
    "remark-gfm": "4.0.0",
    "resolve": "1.22.8",
    "rss-parser": "3.13.0",
    "sanitize-html": "2.12.1",
    "sanitize.css": "13.0.0",
    "shelljs": "0.8.5",
    "styled-components": "6.1.8",
    "ts-node": "10.9.2",
    "typescript": "5.4.2",
    "universalify": "2.0.1",
    "uuid": "9.0.1"
  },
  "scripts": {
    "start": "node ./scripts/start.js",
    "build": "node ./scripts/build.js && cp build/index.html build/404.html",
    "start:prod": "npm run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "npm run eslint src",
    "lint:fix": "npm run eslint src -- --fix",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write --config .prettierrc --ignore-path .prettierignore **/**.{js,jsx,ts,tsx,md,json}",
    "prepare": "is-ci || husky install",
    "lint-staged": "lint-staged"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run checkTs && lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "7.24.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.11",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "20.11.25",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@types/react-redux": "7.1.33",
    "@types/react-router-dom": "5.3.3",
    "@types/sanitize-html": "2.11.0",
    "@types/shelljs": "0.8.15",
    "babel-loader": "9.1.3",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-plugin-styled-components": "2.1.4",
    "babel-preset-react-app": "10.0.1",
    "bfj": "8.0.0",
    "bootstrap": "5.3.3",
    "browserlist": "1.0.1",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "chalk": "5.3.0",
    "cross-env": "7.0.3",
    "css-loader": "6.10.0",
    "css-minimizer-webpack-plugin": "6.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-webpack-plugin": "4.0.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.0",
    "husky": "9.0.11",
    "is-ci": "3.0.1",
    "lint-staged": "15.2.2",
    "mini-css-extract-plugin": "2.8.1",
    "node-plop": "0.32.0",
    "node-polyfill-webpack-plugin": "3.0.0",
    "plop": "4.0.1",
    "postcss": "8.4.35",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "8.1.1",
    "postcss-normalize": "10.0.1",
    "postcss-preset-env": "9.5.0",
    "prettier": "3.2.5",
    "prompts": "2.4.2",
    "react-dev-utils": "12.0.1",
    "resolve-url-loader": "5.0.0",
    "sass": "1.71.1",
    "sass-loader": "14.1.1",
    "semver": "7.6.0",
    "source-map-loader": "5.0.0",
    "style-loader": "3.3.4",
    "stylelint": "16.2.1",
    "stylelint-config-recommended": "14.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "terser-webpack-plugin": "5.3.10",
    "webpack": "5.90.3",
    "webpack-dev-server": "5.0.2",
    "webpack-manifest-plugin": "5.0.0",
    "workbox-webpack-plugin": "7.0.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
